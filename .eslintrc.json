{
  "extends": ["warp/es6"],
  "parserOptions": {
    "sourceType": "script"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "module": true,
    "require": true,
    "global": true
  },
  "rules": {
    "space-before-blocks": [2, "never"],
    "consistent-this": [2, "_this"],
    "semi": [2, "always", {"omitLastInOneLineBlock": true}],
    "curly": 0,
    "func-style": 0,
    "prefer-template": 0,
    "prefer-arrow-callback": 0,
    "no-use-before-define": 0,
    "max-statements-per-line": 0,
    "prefer-rest-params": 0
  }
}
